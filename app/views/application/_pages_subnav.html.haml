.subnavigation
	%nav.container
		= render_navigation do |subnav|
			- subnav.item :sub_user_manual, 'Manuals', "/manuals/", :highlights_on => /\/manuals?\//, :class => 'channel_color_border', :if => Proc.new{ current_user.channel_partner.able_to_see_user_guides? }
			- subnav.item :sub_faq, 'FAQs', faqs_path, :class => 'channel_color_border', :if => Proc.new{ current_user.channel_partner.able_to_see_faqs? }
			-# - subnav.item :sub_support, 'Support Guides', "/supports/", :class => 'channel_color_border', :if => Proc.new{ current_user.channel_partner.able_to_see_supports? }
