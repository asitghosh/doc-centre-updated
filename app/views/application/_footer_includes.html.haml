= javascript_include_tag "application"

- if content_for?(:api_js)
  = yield(:api_js)

- if current_user && ( current_user.has_role?(:appdirect_employee) || current_user.is_impersonating?)
  = javascript_include_tag "see_all"
- if controller_name == 'home'
  = javascript_include_tag "homepage"
/[if lte IE 8]
  = javascript_include_tag "ie"
- if Rails.env.production?
  = javascript_include_tag "google_analytics"
